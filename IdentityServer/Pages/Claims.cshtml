@page
@attribute [Authorize]
@using Microsoft.AspNetCore.Authorization
@model IdentityServer.Pages.ClaimsModel

<table>
  <tr>
    <th>Type</th>
    <th>Value</th>
  </tr>
  @foreach (var claim in User.Claims)
  {
    <tr>
      <td>@claim.Type</td>
      <td>@claim.Value</td>
    </tr>
  }
</table>
